{
    "editor.fontFamily": "'SourceCodePro+Powerline+Awesome Regular','Source Sans Pro', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 14,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 2000,
    "terminal.integrated.fontFamily": "'SourceCodePro+Powerline+Awesome Regular','Source Sans Pro', 'monospace', monospace, 'Droid Sans Fallback'",
    "window.zoomLevel": 0,
    "editor.autoClosingBrackets": true,
    "html.autoClosingTags": false,
    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--ignore=E501,W503"
    ],
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 100,
        "editor.quickSuggestions": false,
    },
    "spellchecker.checkInterval": 10000,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--py36"
    ],
    "window.titleBarStyle": "native",
    "workbench.iconTheme": "vscode-icons",
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "beautify.language": {
        "html": [
            "htm",
            "html",
            "django-html"
        ]
    },
    "[proto3]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "go.formatTool": "goimports",
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "workbench.colorTheme": "Noctis Minimus",
    "cSpell.userWords": [
        "canonicalization",
        "cybersecurty",
        "executables",
        "unencrypted",
        "unintuitive"
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
